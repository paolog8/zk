{
  "database": "Zotero",
  "noteImportFolder": "Reference_Notes",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "png",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite (Author, Year)",
      "format": "formatted-citation",
      "cslStyle": "norsk-apa-manual"
    },
    {
      "name": "Cite LaTeX ",
      "format": "latex",
      "command": "citep[]"
    }
  ],
  "exportFormats": [
    {
      "name": "Import Zotero",
      "outputPathTemplate": "Reference_Notes/{{citekey}}.md",
      "imageOutputPathTemplate": "Reference_Notes/Images/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "Templates/Zotero Template.md",
      "cslStyle": "norsk-apa-manual"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "all-imported-notes",
  "shouldConcat": true,
  "pdfExportImageOCR": true,
  "pdfExportImageTesseractPath": "/usr/bin/tesseract"
}